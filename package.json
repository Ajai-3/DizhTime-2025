{
  "name": "dizhtime-monorepo",
  "version": "1.0.0",
  "description": "DizhTime microservices monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:common": "npm run build --workspace=@dizhtime/common",
    "build:frontend": "cd frontend && tsc && vite build",
    "dev": "cd frontend && vite",
    "dev:common": "npm run dev --workspace=@dizhtime/common",
    "dev:frontend": "cd frontend && vite",
    "dev:user": "cd services/user-service && ts-node src/server.ts",
    "dev:admin": "cd services/admin-service && ts-node src/server.ts",
    "dev:restaurant": "cd services/restaurant-service && ts-node src/server.ts",
    "dev:delivery": "cd services/delivery-boy-service && npx ts-node src/server.ts",
    "dev:chatbot": "cd services/chatbot-service && ts-node src/server.ts",
    "dev:gateway": "cd services/api-gateway && ts-node src/server.ts",
    "start:frontend": "cd frontend && vite",
    "start:services": "concurrently \"npm run dev:user\" \"npm run dev:admin\" \"npm run dev:restaurant\" \"npm run dev:delivery\" \"npm run dev:chatbot\" \"npm run dev:gateway\"",
    "install:all": "npm install --workspaces",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "format": "npm run format --workspaces"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/geolib": "^2.0.0",
    "@types/helmet": "^0.0.48",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.1",
    "@types/node-geocoder": "^3.24.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/redis": "^4.0.0",
    "@types/socket.io": "^3.0.0",
    "@types/socket.io-client": "^3.0.0",
    "@types/stripe": "^8.0.0",
    "@types/supertest": "^6.0.0",
    "@types/twilio": "^3.19.0",
